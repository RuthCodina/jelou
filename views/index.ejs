<div class="author">
    <h1 class="author_heading"> Hi, This is our new Jelou Blog</h1>
    <p class="author_body"> Ruth Codina</p>
</div>
<img src ="/images/jelou.png" alt="enterprises chatbots fintech" class="jelou-image" width="981" height ="528">
<section class ="articles">
    <h2 class="articles_heading">Latest Post</h2>
    <ul class="article-ul">
        <% data.forEach(post => { %>
            <li>
              <a href="/post/<%= post._id %>">
                <span><%= post.title %></span>
                <span class="article-list__date"><%= post.createdAt.toDateString() %></span>
              </a>
            </li>
          <% }) %>
    </ul>
    <a href="#" class="pagination">&lt; View Older Posts</a>
</section>